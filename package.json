{
    "name": "lifesuite-html5",
    "version": "10.7.0",
    "angular-cli": {},
    "scripts": {
        "------------- lifesuite -----------": "",
        "start": "ng serve --configuration=test --proxy-config proxy.conf.json --live-reload false",
        "start:prod": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng serve --configuration=prod-en-us --proxy-config proxy.conf.json --live-reload false",
        "start:prod:fr-ca": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng serve --configuration=prod-fr-ca --proxy-config proxy.conf.json --live-reload false",
        "build": "ng build --configuration=test",
        "build:prod": "npm run build:prod:en-us",
        "build:prod:en-us": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build --configuration=prod-en-us",
        "build:prod:fr-ca": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build --configuration=prod-fr-ca",
        "build:prod:multilang": "npm run build:prod:en-us & npm run build:prod:fr-ca",
        "build:debug": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build --configuration=prod-en-us --verbose",
        "extract-i18n-html": "ng xi18n --output-path locale",
        "extract-i18n-ts": "ngx-extractor --input src/**/*.ts --format=xlf --out-file=src/locale/messages.xlf",
        "extract-i18n": "npm run extract-i18n-html & npm run extract-i18n-ts",
        "merge-i18n": "xliffmerge --profile xliffmerge.json -v",
        "i18n": "./node_modules/.bin/ng-xi18n --i18nFormat=xlf",
        "lint": "tslint \"src/**/*.ts",
        "------------- components-lab -----------": "",
        "start:components-lab": "ng serve components-lab",
        "build:components-lab": "ng build components-lab",
        "build:components-lab:prod": "npm run build:components-lab:prod:en-us",
        "build:components-lab:prod:en-us": "ng build components-lab --configuration=prod-en-us",
        "build:components-lab:prod:fr-ca": "ng build components-lab --configuration=prod-fr-ca",
        "build:components-lab:prod:multilang": "npm run build:components-lab:prod:en-us & npm run build:components-lab:prod:fr-ca",
        "extract-i18n-html:components-lab": "ng xi18n components-lab --output-path src/locale",
        "extract-i18n-ts:components-lab": "ngx-extractor --input projects/components-lab/**/*.ts --format=xlf --out-file=projects/components-lab/src/locale/messages.xlf",
        "extract-i18n:components-lab": "npm run extract-i18n-html:components-lab & npm run extract-i18n-ts:components-lab",
        "merge-i18n:components-lab": "xliffmerge --profile projects/components-lab/xliffmerge.json -v",
        "------------- tests -----------": "",
        "test": "ng test",
        "pree2e": "webdriver-manager update --standalone false --gecko false",
        "e2e": "protractor",
        "------------- analyze bundles -----------": "",
        "sme": "./node_modules/.bin/source-map-explorer",
        "gen-status": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build --prod --aot -sm --stats-json",
        "gen-report": "webpack-bundle-analyzer ../LifeSuite.Web/wwwroot/stats.json"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "7.0.2",
        "@angular/common": "7.0.2",
        "@angular/compiler": "7.0.2",
        "@angular/core": "7.0.2",
        "@angular/forms": "7.0.2",
        "@angular/http": "7.0.2",
        "@angular/platform-browser": "7.0.2",
        "@angular/platform-browser-dynamic": "7.0.2",
        "@angular/router": "7.0.2",
        "@aspnet/signalr": "1.0.0",
        "@kendo/kendo-angular-intl": "file:Scripts/kendo/NpmImages/kendo-angular-intl",
        "@ng-bootstrap/ng-bootstrap": "4.0.0",
        "@ng-idle/core": "6.0.0-beta.3",
        "@ng-idle/keepalive": "6.0.0-beta.3",
        "@ng-select/ng-select": "2.12.1",
        "@ngrx/core": "1.2.0",
        "@ngrx/store": "6.1.0",
        "@ngx-translate/i18n-polyfill": "0.2.0",
        "ag-grid-angular": "19.0.0",
        "ag-grid-community": "19.0.0",
        "angular-split": "1.0.0-rc.3",
        "bootstrap": "4.0.0",
        "card-validator": "4.1.1",
        "cldr-data": "31.0.2",
        "core-js": "2.5.4",
        "font-awesome": "4.7.0",
        "ng2-cookies": "1.0.12",
        "primeicons": "1.0.0",
        "primeng": "6.1.5",
        "reflect-metadata": "0.1.10",
        "rxjs": "6.3.3",
        "rxjs-compat": "6.3.3",
        "smoothscroll-polyfill": "0.4.3",
        "text-security": "1.1.0",
        "ts-helpers": "1.1.2",
        "web-animations-js": "2.3.1",
        "yargs": "6.6.0",
        "zone.js": "0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.10.3",
        "@angular/cli": "7.0.4",
        "@angular/compiler-cli": "7.0.2",
        "@types/jasmine": "2.5.40",
        "@types/jquery": "2.0.48",
        "@types/node": "7.0.32",
        "@types/smoothscroll-polyfill": "0.3.0",
        "angular-mocks": "1.6.4",
        "codelyzer": "4.0.1",
        "jasmine-core": "3.2.0",
        "jasmine-spec-reporter": "4.2.1",
        "karma": "6.3.14",
        "karma-chrome-launcher": "2.2.0",
        "karma-coverage-istanbul-reporter": "2.0.1",
        "karma-ie-launcher": "1.0.0",
        "karma-jasmine": "1.1.2",
        "karma-jasmine-html-reporter": "1.3.0",
        "karma-remap-istanbul": "^0.6.0",
        "ngx-i18nsupport": "0.16.2",
        "protractor": "^5.4.1",
        "source-map-explorer": "1.6.0",
        "ts-node": "3.2.0",
        "tslint": "5.7.0",
        "typescript": "3.1.6",
        "webpack-bundle-analyzer": "2.13.1"
    }
}
