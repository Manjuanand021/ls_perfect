            this.messagingService.closeChannel(_lf_split_pane_channels__WEBPACK_IMPORTED_MODULE_6__["LfSplitPaneChannels"].SplitPaneHidden);
        }
        this.saveSplitPaneState();
    };
    SplitPaneContainerViewModel.prototype.getErrorItemMessage = function (areaId) {
        return this._splitPaneContainerMessagesMapper.getMessage(areaId);
    };
    SplitPaneContainerViewModel.prototype.handleSplitGutterClick = function (e) {
        var SplitPaneMinimizedSize = 0.5;
        if (this.splitterVisible) {
            this.splitterMinimized = !this.splitterMinimized;
            this.splitPaneSize = this.splitterMinimized ? SplitPaneMinimizedSize : this.splitPaneInitialSize;
            this.setContainerAreasSize(this.splitPaneSize);
            this.containerAreasInfo.right.size = this.splitPaneSize;
        }
    };
    SplitPaneContainerViewModel.prototype.handleSplitGutterDoubleClick = function (e) { };
    SplitPaneContainerViewModel.prototype.createStateValueAccessor = function (tabStateManager) {
        this._splitPaneStateValueAccessor = new life_core_util__WEBPACK_IMPORTED_MODULE_2__["TabStateValueAccessor"](tabStateManager, this.splitPaneStateValueKey);
    };
    SplitPaneContainerViewModel.prototype.saveSplitPaneState = function () {
        if (this.splitterVisible) {
            var splitPaneStateData = new _lf_split_pane_model__WEBPACK_IMPORTED_MODULE_5__["SplitPaneStateData"]();
            splitPaneStateData.splitPaneSize = this.splitPaneSize;
            splitPaneStateData.splitPaneAreas = this.splitPaneAreas;
            this._splitPaneStateValueAccessor.setValue(splitPaneStateData);
        }
    };
    SplitPaneContainerViewModel.prototype.restoreSplitPaneState = function (splitPaneStateData) {
        this.splitPaneSize = splitPaneStateData.splitPaneSize;
        this.setContainerAreasSize(this.splitPaneSize);
        this.splitPaneAreas = splitPaneStateData.splitPaneAreas;
        this.setContainerSplitVisible(this.splitPaneAreas.length > 0);
    };
    SplitPaneContainerViewModel.prototype.initSubscribers = function () {
        var _this = this;
        this.subscriptionTracker.track(this.messagingService.subscribe(_lf_split_pane_channels__WEBPACK_IMPORTED_MODULE_6__["LfSplitPaneChannels"].AddSplitPaneArea, function (value) {
            if (_this.addSplitArea(value)) {
                _this.publishSplitPaneVisibleMessage();
            }
        }));
        this.subscriptionTracker.track(this.messagingService.subscribe(_lf_split_pane_channels__WEBPACK_IMPORTED_MODULE_6__["LfSplitPaneChannels"].RemoveSplitPaneArea, function (splitPaneResult) {
            _this.removeSplitArea(splitPaneResult);
            _this.publishSplitPaneHiddenMessage();
        }));
    };
    SplitPaneContainerViewModel.prototype.publishSplitPaneVisibleMessage = function () {
        if (this.isSplitPaneVisible()) {
            this.messagingService.publish(_lf_split_pane_channels__WEBPACK_IMPORTED_MODULE_6__["LfSplitPaneChannels"].SplitPaneVisible);
        }
    };
    SplitPaneContainerViewModel.prototype.publishSplitPaneHiddenMessage = function () {
        if (this.isSplitPaneHidden()) {
            this.messagingService.publish(_lf_split_pane_channels__WEBPACK_IMPORTED_MODULE_6__["LfSplitPaneChannels"].SplitPaneHidden);
        }
    };
    SplitPaneContainerViewModel.prototype.isSplitPaneVisible = function () {
        return this.splitPaneAreas.length === 1;
    };
    SplitPaneContainerViewModel.prototype.isSplitPaneHidden = function () {
        return this.splitPaneAreas.length === 0;
    };
    SplitPaneContainerViewModel.prototype.splitPaneTypeAlreadyExists = function (areaId) {
        var areaType = this.getSplitAreaType(areaId);
        return this.splitPane && this.splitPane.hasSplitArea(areaId, areaType);
    };
    SplitPaneContainerViewModel.prototype.showSplitAreaExistMessage = function (areaId) {
        this._confirmDialog.open({
            message: this.getErrorItemMessage(areaId),
            title: this.getErrorItemDialogTitle(),
            buttons: [new life_core_component__WEBPACK_IMPORTED_MODULE_7__["DialogButton"]({ type: life_core_component__WEBPACK_IMPORTED_MODULE_7__["DialogButtonType"].OK })]
        });
    };
    SplitPaneContainerViewModel.prototype.initContainerAreas = function () {
        this.splitPaneSize = this.splitPaneInitialSize;
        this.containerAreasInfo = this.getContainerAreasInfo();
        this.setContainerAreasSize(this.splitPaneInitialSize);
    };
    SplitPaneContainerViewModel.prototype.initClickEventResolver = function () {
        var _this = this;
        this._gutterClickEventResolver = new life_core_component_shared_event_click_event_resolver__WEBPACK_IMPORTED_MODULE_3__["ClickEventResolver"](function (event) { return _this.handleSplitGutterClick(event); }, function (event) { return _this.handleSplitGutterClick(event); });
    };
    SplitPaneContainerViewModel.prototype.showSplitter = function () {
        this.setContainerSplitVisible(true);
    };
    SplitPaneContainerViewModel.prototype.hideSplitter = function () {
        this.setContainerSplitVisible(false);
    };
    SplitPaneContainerViewModel.prototype.setContainerSplitVisible = function (visible) {
        if (visible) {
            this.splitterVisible = true;
            this.splitContainerClass = 'tab-split-container';
        }
        else {
            this.splitterVisible = false;
            this.splitContainerClass = 'tab-no-split-container';
        }
    };
