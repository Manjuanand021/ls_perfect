            _this.getReviewerList().then(function () {
                _this.setDefaultReviewer();
                _this._addCaseNoteListDataResolver.context = undefined;
            });
        });
    };
    AddCaseNoteComponent.prototype.onApplicantChange = function (activeApplicant) {
        if (activeApplicant && activeApplicant.value) {
            this._activeApplicantHelper.setActiveApplicantId(activeApplicant.value);
        }
    };
    AddCaseNoteComponent.prototype.onNeedReviewChange = function (value) {
        if (value) {
            this.data.DiaryDate = this.getTodaysDate();
            this.data.EmailToList = ls_core_component_input__WEBPACK_IMPORTED_MODULE_8__["LsRadioButtonValues"].NO_STRING;
        }
        else {
            this.data.DiaryDate = null;
            this.data.EmailToList = '';
            this.data.ReferToUserId = -1;
            this.data.UserType = 0;
        }
    };
    AddCaseNoteComponent.prototype.onDialogButtonClick = function (buttonId) {
        var _this = this;
        if (buttonId === life_core_component_dialog__WEBPACK_IMPORTED_MODULE_2__["DialogButtonType"].ACCEPT) {
            return this.validate().then(function (result) {
                if (result == life_core_view_model__WEBPACK_IMPORTED_MODULE_1__["ViewValidationResult"].pass) {
                    _this.addNote();
                    if (_this.needReview) {
                        return _this.handleNoteReview(result);
                    }
                    if (!_this.needReview && _this.isSupplementalNote) {
                        _this.undoReviewOnParentNote(); // blindly assuming that supplemental note is added because of review on parent note
                    }
                    return new life_core_component_dialog__WEBPACK_IMPORTED_MODULE_2__["DialogViewModelResult"](_this.data, result === life_core_view_model__WEBPACK_IMPORTED_MODULE_1__["ViewValidationResult"].pass, _this.isDirty());
                }
            });
        }
        else {
            return Promise.resolve(null);
        }
    };
    AddCaseNoteComponent.prototype.undoReviewOnParentNote = function () {
        this.parentNote.DiaryDate = null;
        this.parentNote.UserType = 0;
        this.parentNote.ReferToUserId = -1;
        this.parentNote.EmailToList = ls_core_component_input__WEBPACK_IMPORTED_MODULE_8__["LsRadioButtonValues"].NO_STRING;
    };
    AddCaseNoteComponent.prototype.handleNoteReview = function (result) {
        var _this = this;
        return this.getReviewerAsUserDTO().then(function (reviewerAsUserDTO) {
            _this.data.Topic = _this.getNoteTopic(reviewerAsUserDTO);
            if (_this.data.EmailToList === ls_core_component_input__WEBPACK_IMPORTED_MODULE_8__["LsRadioButtonValues"].YES_STRING) {
                _this.sendEmail(_this.getReviewerEmailAddress(reviewerAsUserDTO));
            }
            return new life_core_component_dialog__WEBPACK_IMPORTED_MODULE_2__["DialogViewModelResult"](_this.data, result === life_core_view_model__WEBPACK_IMPORTED_MODULE_1__["ViewValidationResult"].pass, _this.isDirty());
        });
    };
    AddCaseNoteComponent.prototype.getNoteTopic = function (reviewerAsUserDTO) {
        if (reviewerAsUserDTO && !this.isSupplementalNote) {
            return this.buildReferredToNoteTopic(reviewerAsUserDTO.FirstName, reviewerAsUserDTO.LastName);
        }
        else if (this.isSupplementalNote) {
            return this.parentNote.Topic;
        }
        else if (!this.isSupplementalNote) {
            return this.data.Topic;
        }
    };
    AddCaseNoteComponent.prototype.getReviewerEmailAddress = function (reviewerAsUserDTO) {
        return reviewerAsUserDTO ? reviewerAsUserDTO.EmailAddress : '';
    };
    AddCaseNoteComponent.prototype.getReviewerAsUserDTO = function () {
        if (this.data.ReferToUserId) {
            var request = this.getReviewerAsUserDTORequest();
            var serviceParams = this.getServiceParams(request);
            return this._dataService.getData(serviceParams).then(function (response) {
                var reviewerAsUserResponse = response.responsePayload;
                return reviewerAsUserResponse ? reviewerAsUserResponse.dtoObject : null;
            });
        }
        return Promise.resolve(null);
    };
    AddCaseNoteComponent.prototype.getReviewerAsUserDTORequest = function () {
        var reviewer = new ls_core_model__WEBPACK_IMPORTED_MODULE_10__["UserDTO"]();
        reviewer.UserId = this.data.ReferToUserId;
        var request = new ls_core_service_load_bypk__WEBPACK_IMPORTED_MODULE_7__["DTOLoadByPKRequest"]();
        request._parentDTOObject = null;
        request._workingDTOObject = reviewer;
        request._eagerLoad = true;
        return request;
    };
    AddCaseNoteComponent.prototype.getServiceParams = function (request) {
        return new ls_core_service__WEBPACK_IMPORTED_MODULE_5__["DataServiceParams"]({
            serviceInterface: ls_core_service__WEBPACK_IMPORTED_MODULE_5__["UIServiceNames"].DTO_LOAD,
            serviceMethod: ls_core_service__WEBPACK_IMPORTED_MODULE_5__["UIServiceMethods"].DTO_LOAD,
            requestPayload: request
        });
    };
    AddCaseNoteComponent.prototype.addNote = function () {
        this.data.AddedDate = this.getTodaysDate();
        this.data.UpdateDate = this.getTodaysDate();
        this.data.UpdatedBy = this.user.UserId;
        this.setPolicyPersonID();
        this.data.NoteType = this.getNoteType();
        if (this.isSupplementalNote) {
            this.data.PrimaryAuthor = this.parentNote.PrimaryAuthor;
        }
        else {
            this.data.PrimaryAuthor = this.user.UserId;
        }
        this.data.SupplementalNotes = [];
        this.updateUserApplicationCounts();
        this.changeManager.setIsDirty(true);
    };
    AddCaseNoteComponent.prototype.setPolicyPersonID = function () {
        var insured = this.getInsured();
        if (insured) {
            this.data.PolicyPersonId = insured.PolicyPersonId.toString();
        }
    };
    AddCaseNoteComponent.prototype.getTodaysDate = function () {
        return ls_core_util__WEBPACK_IMPORTED_MODULE_6__["DBDateUtil"].dateToDBDate(new Date());
    };
    AddCaseNoteComponent.prototype.getNoteType = function () {
        return this.needReview ? 'Refer' : 'Manual';
    };
    AddCaseNoteComponent.prototype.getInsured = function () {
        var _this = this;
        return this.policy.Insureds_LazyLoad.find(function (insured) { return insured.PersonId == _this.applicantId; });
    };
    AddCaseNoteComponent.prototype.updateUserApplicationCounts = function () {
        this._applicationCountersHelper.incrementManualActionCounter(this.policy);
    };
    AddCaseNoteComponent.prototype.buildReferredToNoteTopic = function (reviewerFirstName, reviewerLastName) {
        var formattedReviewerName = this._nameFormatter.format(life_core_util__WEBPACK_IMPORTED_MODULE_4__["NamePattern"].LastAndFirstName, reviewerFirstName, reviewerLastName);
        return "Referred to: " + formattedReviewerName + " - " + this.data.Topic;
    };
    AddCaseNoteComponent.prototype.sendEmail = function (emailAddress) {
        this._emailUtil.sendEmail(emailAddress, this.data.Topic, this.buildEmailBody());
    };
    AddCaseNoteComponent.prototype.buildEmailBody = function () {
        return "Case Number: " + this.policy.PolicyNumber + " %0D%0AInsured Name: " + this.getInsured().LastName + ", " + this.getInsured().FirstName + " %0D%0ANote text: " + this.data.Note;
    };
    AddCaseNoteComponent.prototype.getState = function () {
        var caseNoteModel = new CaseNoteStateModel();
        caseNoteModel.needsReview = this.needReview;
        caseNoteModel.reviewerListOptions = this.reviewerListOptions;
        caseNoteModel.note = this.data;
        return caseNoteModel;
    };
    AddCaseNoteComponent.prototype.getAuthorName = function (note) {
        var author = note.PrimaryAuthor ? note.PrimaryAuthor : note.Author;
        return author ? life_core_util__WEBPACK_IMPORTED_MODULE_4__["ListUtil"].getLabelByValue(this._resolvedData.metaData.aus_users, author.toString()) : '';
    };
    AddCaseNoteComponent.prototype.getReferredToUserName = function (note) {
        var referredToUser = note.ReferToUserId ? note.ReferToUserId : null;
        return referredToUser
            ? life_core_util__WEBPACK_IMPORTED_MODULE_4__["ListUtil"].getLabelByValue(this._resolvedData.metaData.aus_users, referredToUser.toString())
            : '';
    };
    AddCaseNoteComponent.prototype.setNoteFieldLabel = function () {
        this.noteFieldLabel = this.isSupplementalNote
            ? this.i18n({ value: 'Supplemental Note', id: 'policy.notes.addnotes.supplementalnote.label' })
            : this.i18n({ value: 'Note', id: 'policy.notes.addnotes.note.label' });
    };
