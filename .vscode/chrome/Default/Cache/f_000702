/***/ "./src/lifesuite-app/ls-core/model/auto-navigation/navigation-sequence.ts":
/*!********************************************************************************!*\
  !*** ./src/lifesuite-app/ls-core/model/auto-navigation/navigation-sequence.ts ***!
  \********************************************************************************/
/*! exports provided: NavigationSequence */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "NavigationSequence", function() { return NavigationSequence; });
var NavigationSequence = /** @class */ (function () {
    function NavigationSequence() {
        this._navigationTargets = [];
    }
    NavigationSequence.prototype.addNavigationTarget = function (item) {
        this._navigationTargets.push(item);
        return item;
    };
    /**
     * Returns clone of navigation sequence
     * @return cloned NavigationSequence object
     *
     */
    NavigationSequence.prototype.clone = function () {
        var sequence = new NavigationSequence();
        sequence._navigationTargets = this._navigationTargets.slice();
        return sequence;
    };
    /**
     * Returns next navigation target from the navigation sequence
     * @return navigation target
     *
     */
    NavigationSequence.prototype.getNextNavigationTarget = function () {
        return this._navigationTargets.length > 0 ? this._navigationTargets[0] : null;
    };
    Object.defineProperty(NavigationSequence.prototype, "length", {
        /**
         * Length of navigation sequence
         * @return length value
         *
         */
        get: function () {
            return this._navigationTargets.length;
        },
        enumerable: true,
        configurable: true
    });
    /**
     * Removes navigation target from navigation sequence
     * @param item - navigation target object to remove
     *
     */
    NavigationSequence.prototype.removeNavigationTarget = function (item) {
        var removeIndex = this._navigationTargets.findIndex(function (target) { return target.navigatorId === item.navigatorId; });
        this._navigationTargets.splice(removeIndex, 1);
    };
    return NavigationSequence;
}());



/***/ }),

/***/ "./src/lifesuite-app/ls-core/model/auto-navigation/navigation-target.ts":
/*!******************************************************************************!*\
  !*** ./src/lifesuite-app/ls-core/model/auto-navigation/navigation-target.ts ***!
  \******************************************************************************/
/*! exports provided: NavigationTarget, NavigationTargetTypes */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "NavigationTarget", function() { return NavigationTarget; });
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "NavigationTargetTypes", function() { return NavigationTargetTypes; });
/**
 * NavigationTarget defines a single navigation step in auto-navigation sequence
 *
 */
var NavigationTarget = /** @class */ (function () {
    function NavigationTarget() {
        this._params = new Map();
    }
    Object.defineProperty(NavigationTarget.prototype, "navigatorId", {
        /**
         * Id of the class implementing navigation step
         * described in this navigation target
         * @return class Id
         *
         */
        get: function () {
            return this._navigatorId;
        },
        set: function (value) {
            this._navigatorId = value;
        },
        enumerable: true,
        configurable: true
    });
    Object.defineProperty(NavigationTarget.prototype, "targetType", {
        /**
         * Type of the navigation target: Tab, SubTab, Popup, etc.
         * @return
         *
         */
        get: function () {
            return this._targetType;
        },
        set: function (value) {
            this._targetType = value;
        },
        enumerable: true,
        configurable: true
    });
    /**
     * Adds parameter to the collection describing navigation target
     * @param key
     * @param value
     * @return boolean value indicating whether parameter was added successfully
     *
     */
    NavigationTarget.prototype.addParam = function (key, value) {
        if (key == null || value == null || this._params.has(key)) {
            return false;
        }
        else {
            this._params.set(key, value);
            return true;
        }
    };
    /**
     * Removes parameter from the collection describing navigation target
     * @param key
     * @return parameter being removed
     *
     */
    NavigationTarget.prototype.removeParam = function (key) {
        var value = '';
        if (this._params.has(key)) {
            value = this._params.get(key);
            this._params.delete(key);
        }
        return value;
    };
    /**
     * Retrieves parameter from the collection describing navigation target
     * @param key
     * @return parameter that corresponds to the provided key
     *
     */
    NavigationTarget.prototype.getParam = function (key) {
        var value = '';
        if (this._params.has(key)) {
            value = this._params.get(key);
        }
        return value;
    };
    NavigationTarget.PARAM_TARGET_ID = 'targetId';
    NavigationTarget.PARAM_PERSON_ID = 'personId';
    NavigationTarget.PARAM_COLLECTION_ITEM_ID = 'collectionItemId';
    return NavigationTarget;
}());

var NavigationTargetTypes;
(function (NavigationTargetTypes) {
    NavigationTargetTypes["Tab"] = "targetTypeTab";
    NavigationTargetTypes["SubTab"] = "targetTypeSubTab";
    NavigationTargetTypes["Popup"] = "targetTypePopup";
    NavigationTargetTypes["CollectionItem"] = "targetTypeCollectionItem";
    NavigationTargetTypes["Route"] = "targetTypeRoute";
})(NavigationTargetTypes || (NavigationTargetTypes = {}));


/***/ })

}]);
