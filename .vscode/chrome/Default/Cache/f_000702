        }
    };
    AddCaseNoteComponent.prototype.onReviewerTypeChange = function (reviewerTypeSelection) {
        var _this = this;
        this.getReviewerList().then(function () { return _this.setDefaultReviewer(); });
    };
    AddCaseNoteComponent.prototype.getReviewerList = function () {
