    ScratchPadDataService.prototype.updateScratchPad = function (scratchPadDTO) {
        var _this = this;
        var serviceParams = this.getServiceParams(scratchPadDTO);
        return this._dataService.updateData(serviceParams).then(function (response) {
            // Below is temporary, it is used to check response. In future it will be removed.
            if (response.responsePayload) {
                _this._logger.log('Scratch Pad data saved successfully.');
            }
