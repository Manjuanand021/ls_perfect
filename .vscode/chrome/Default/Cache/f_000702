    }
    SplitPaneContainerViewModel.prototype.loadData = function () {
        var splitPaneStateData = this._splitPaneStateValueAccessor.getValue();
        if (splitPaneStateData) {
            this.restoreSplitPaneState(splitPaneStateData);
        }
        else {
            this.setContainerSplitVisible(false);
        }
        return Promise.resolve();
    };
    SplitPaneContainerViewModel.prototype.addSplitArea = function (areaData) {
        if (this.splitPaneTypeAlreadyExists(areaData.areaId)) {
            this.showSplitAreaExistMessage(areaData.areaId);
            return;
        }
        if (!this.splitterVisible) {
            this.showSplitter();
        }
        var splitPaneArea = this.createSplitPaneArea(areaData.areaId);
        splitPaneArea.index = this.splitPaneAreas.length + 1;
        splitPaneArea.context = areaData.context;
        this.splitPaneAreas.push(splitPaneArea);
        return splitPaneArea;
    };
    SplitPaneContainerViewModel.prototype.removeSplitArea = function (splitPaneResult) {
        var area = this.splitPaneAreas.find(function (area) { return area.id == splitPaneResult.areaId; });
        if (area) {
            area.context = undefined;
            this.splitPaneAreas.splice(this.splitPaneAreas.indexOf(area), 1);
            if (this.splitPaneAreas.length == 0) {
                this.hideSplitter();
            }
        }
    };
    SplitPaneContainerViewModel.prototype.onSplitGutterDragStart = function (e) {
        this.gutterColor = _lf_split_model__WEBPACK_IMPORTED_MODULE_4__["GutterColor"].drag;
        this.splitPaneSize = e.sizes[1];
    };
    SplitPaneContainerViewModel.prototype.onSplitGutterDragEnd = function (e) {
        this.gutterColor = _lf_split_model__WEBPACK_IMPORTED_MODULE_4__["GutterColor"].default;
        this.splitPaneSize = e.sizes[1];
    };
    SplitPaneContainerViewModel.prototype.onSplitGutterClick = function (e) {
        this.gutterColor = _lf_split_model__WEBPACK_IMPORTED_MODULE_4__["GutterColor"].default;
        this._gutterClickEventResolver.onClick(e);
    };
    SplitPaneContainerViewModel.prototype.ngOnDestroy = function () {
        _super.prototype.ngOnDestroy.call(this);
        this.messagingService.closeChannel(_lf_split_pane_channels__WEBPACK_IMPORTED_MODULE_6__["LfSplitPaneChannels"].AddSplitPaneArea);
        this.messagingService.closeChannel(_lf_split_pane_channels__WEBPACK_IMPORTED_MODULE_6__["LfSplitPaneChannels"].RemoveSplitPaneArea);
        if (this.messagingService.channelExist(_lf_split_pane_channels__WEBPACK_IMPORTED_MODULE_6__["LfSplitPaneChannels"].SplitPaneVisible)) {
            this.messagingService.closeChannel(_lf_split_pane_channels__WEBPACK_IMPORTED_MODULE_6__["LfSplitPaneChannels"].SplitPaneVisible);
        }
